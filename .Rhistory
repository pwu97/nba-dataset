labels = c("1960s","1960s", "1970s", "1980s", "1990s", "2000s", "2010s")))
colnames(nfl_year)[colnames(nfl_year) == "cut(...)"] <- 'Decades'
nfl_year <- mutate(nfl_year, "total_score"= home_score+away_score,
"margin_victory" = abs(home_score-away_score))
print(nfl_year)
