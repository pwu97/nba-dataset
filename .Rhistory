filter(!is.na(Decades)) %>%
filter(!is.na(temperature)) %>%
filter(!is.na(wind_mph)) %>%
mutate(date = as.character(date)) %>%
mutate(year = as.numeric(str_sub(date, -4, -1))) %>%
mutate(humidity = as.numeric(str_sub(humidity, 1, -2))) %>%
filter(!is.na(humidity))
nfl_year %>%
mutate(total_score = away_score + home_score) %>%
group_by(year) %>%
summarize(avg_total_score = sum(total_score)/n()) %>%
ungroup() %>%
ggplot(aes(x = year, y = avg_total_score)) +
geom_point() +
geom_smooth() +
scale_x_continuous(breaks = c(seq(1950, 2030, 10))) +
theme_bw() +
labs(x = "Year",
